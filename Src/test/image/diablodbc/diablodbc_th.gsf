# diablodbc
# Title: Sample input for plane strain cantilever beam ala DLEARN
# Min/Max Switch: global

# Setup
# alias chgst f;n;n;l // Enable in Release 13.1
alias min 1
alias max 3
alias staterange minst min; maxst max
clrsel 
switch mglob
# staterange // Enable in Release 13.1
# chgst // Enable in Release 13.1
minst 1
maxst 81
on thsinglecol
fracsz 8
jpegqual 100

# Object selection 
select brick 1 45 48 116 80 68 97-100
select node 1 11 100 225 17-22
unselect node 20 21
select mat 1

# Append output to TH file
on tha

# Plot for variable nodpos[uy]
plot nodpos[uy]
show  nodpos[uy]
outth diablodbc_th.jansw cur

select node 5
# Plot for variable accmag
plot accmag
show  accmag
outth diablodbc_th.jansw cur

# Plot and Images for variable sx
show sx
plot
outth diablodbc_th.jansw cur

# Plot for variable sz
show sz
outth diablodbc_th.jansw

# Plot for variable sxy
show sxy
plot
outth diablodbc_th.jansw

# Plot for variable szx
show szx
plot
outth diablodbc_th.jansw

# Plot for variable evol
show evol
plot
outth diablodbc_th.jansw

# Plot for variable gamxy
show gamxy
plot
outth diablodbc_th.jansw

# Plot for variable dispmag
plot dispmag
outth diablodbc_th.jansw

echo "Writing TH data to file diablodbc_th.jansw"

# EndCase 

