# plate71
# Title: Pressure loaded plate, shell elements
# Material Model: 71
# Units: gm-cm-ms
# End Time: 500000 (State 101)
# Griz Script File: plate71_th.gsf
# Min/Max Switch: global

# Setup
# alias chgst f;n;n;l // Enable after Release 13.1
alias min 1
alias max 11
alias staterange minst min; maxst max
clrsel 
switch mglob
# staterange; chgst // Enable after Release 13.1
minst 1
maxst 11

on thsinglecol
fracsz 10
jpegqual 100

off all; on cmap; on title; on time; on coord
switch mglob

# Object selection 
select mat 1

# Append output to TH file
on tha

refstate 10

#  Plot for variable dispz
clrsel 
minst 1
maxst 101
select node 1 11 121 111 61 49 97 73
plot dispz
outth plate71_th.jansw cur

#  Plot for variable nodpos[uy]
plot nodpos[uy]
outth plate71_th.jansw cur

#  Plot for variable dispy
show dispy
outth plate71_th.jansw cur

# Plot for variable nodpos[ux]
show nodpos[ux]
outth plate71_th.jansw cur

# Object Selection
clrsel
select shell 1 100 91 56 10 35 50-51 76

# Plot for variable sxy
show sxy; plot
outth plate71_th.jansw cur

# Plot for variable szx
show szx; plot
outth plate71_th.jansw cur

# Plot for variable press
show press; plot
outth plate71_th.jansw cur

# Plot for variable bend[myy]
show bend[myy]; plot
outth plate71_th.jansw cur

# Plot for variable normal[nxy]
show normal[nxy];    plot
outth plate71_th.jansw cur

# EndCase 
