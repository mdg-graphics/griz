# ntet20
# Title: Simple meshless particle example of bar impact (same problem as bar71)
# Material Model: 3
# Units: gm-cm-ms
# End Time: 80
# Griz Script File: ntet20.gsf
# XY Plot File Prefix: plot_ntet20_
# XY Data File Prefix: xy_ntet20_
# Image File Prefix: image_ntet20_
# Min/Max Switch: global
# Verbatim To Griz: off all; on cmap; on title; on time; on coord
off all; on cmap; on title; on time; on coord
switch mglob
#  Plot for variable eps
clrsel 
select ntet 1 2 261
minst 1
maxst 81
plot eps
outjpeg plot_ntet20_eps.jpg
outth xy_ntet20_eps cur
#  Plot for variable nodepos[uz]
clrsel 
select ntet 1 2 261
minst 1
maxst 81
select node 1
plot nodepos[uz]
outjpeg plot_ntet20_nodepos[uz].jpg
outth xy_ntet20_nodepos[uz] cur
#  Image for variable sx
clrsel 
time 80
rview; ry 90; rx 45; ry -35.26; rz -30
exclude all; include 1
show sx
outjpeg image_ntet20_sx.jpg
#  Image for variable sz
clrsel 
time 80
rview; ry 90; rx 45; ry -35.26; rz -30
show sz
outjpeg image_ntet20_sz.jpg
#  Image for variable szx
clrsel 
time 80
rview; ry 90; rx 45; ry -35.26; rz -30
show szx
outjpeg image_ntet20_szx.jpg
#  Image for variable eps
clrsel 
time 80
rview; ry 90; rx 45; ry -35.26; rz -30
show eps
outjpeg image_ntet20_eps.jpg
# EndCase 
